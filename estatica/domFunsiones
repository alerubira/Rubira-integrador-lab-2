let ocultar=document.getElementsByClassName('ocultar');
let limpiar=document.getElementsByClassName('limpiar');
 function Focultar(){
            
        for (let elemento of ocultar) {
            elemento.style.display = 'none';
                }
        }
function limpiarCampos(list){
    //console.log(list);
    for (let li of list){
        li.textContent="";
        li.value=null;
    }
}
//export{Focultar};

